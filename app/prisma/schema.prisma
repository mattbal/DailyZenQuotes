generator client {
  provider = "prisma-client"
  output = "../src/app/lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Email {
  email String @id 
  encryptedEmail String
}

model Quote {
  id Int @id @default(autoincrement())
  quote String
  author String
}

model Index {
  index Int @id
}