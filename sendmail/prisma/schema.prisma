generator client {
  provider = "prisma-client"
  output = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Email {
  email String @id 
  encryptedEmail String
}

model Quote {
  id Int @id @default(autoincrement())
  quote String
  author String
}

model Index {
  index Int @id
}